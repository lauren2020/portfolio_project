            <textarea id="cipherTextField" class="cipher-field" name="firstname"></textarea>
            <input type="submit" value="Decipher" onclick="decipher()">

            <div class="dropdown">
                <button onclick="openColumnarTranspositionCipherMenu()" class="dropbtn">Col. Transposition Cipher Tools</button>
                <div id="columnarTranspositionCipherDropdown" class="cipher-dropdown-content">
