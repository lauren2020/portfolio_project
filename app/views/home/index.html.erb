                <button onclick="openAffineCipherMenu()" class="dropbtn">Affine Cipher Tools</button>
                <div id="affineCipherDropdown" class="cipher-dropdown-content">
                    <input type="submit" value="Affine Decipher" onclick="affineDecipherUnknownAB()">
                    <input type="submit" value="Affine Decipher (a and b)" onclick="affineDecipher1()">
                </div>
            </div>
            <!--<input type="submit" value="Affine Decipher" onclick="affineDecipherUnknownAB()">
            <input type="submit" value="Affine Decipher (a and b)" onclick="affineDecipher1()">-->

            <div class="dropdown">
                <button onclick="openShiftCipherMenu()" class="dropbtn">Shift Cipher Tools</button>
                <div id="shiftCipherDropdown" class="cipher-dropdown-content">
                    <input type="submit" value="Shift Decipher (Shift a)" onclick="shiftDecipherOfAlength()">
                    <input type="submit" value="Permeate All Shifts" onclick="permeateAllShifts()">
                </div>
            </div>
            <!--<input type="submit" value="Shift Decipher (Shift a)" onclick="shiftDecipherOfAlength()">
